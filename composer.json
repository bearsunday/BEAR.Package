{
    "name":"bear/package",
    "type":"library",
    "description":"BEAR.Sunday application package",
    "version":"0.6.0",
    "keywords":[
        "BEAR.Sunday",
        "application skeleton",
        "Smarty3",
        "Doctrine DBAL"
    ],
    "homepage":"https://github.com/koriym/BEAR.Sunday",
    "license":"BSD-3-Clause",
    "authors":[
        {
            "name":"Akihito Koriyama",
            "email":"akihito.koriyama@gmail.com"
        }
    ],
    "require":{
        "php":">=5.4.0",
        "ext-apc":"*",
        "ext-curl":"*",
        "ray/aop":"1.0.*",
        "ray/di":"1.0.*",
        "bear/resource":"0.6.*",
        "bear/sunday":"0.6.*",
        "aura/di":"1.0.*",
        "aura/router":"1.0.*",
        "aura/signal":"1.0.*",
        "aura/web":"1.0.*",
        "aura/installer-default":"1.0.*",
        "doctrine/common":"2.3.*-dev",
        "doctrine/dbal":"2.3.*-dev",
        "guzzle/guzzle":"3.0.*",
        "pagerfanta/pagerfanta":"dev-master",
        "smarty/smarty":"3.1.*",
        "symfony/console":"2.0.*",
        "symfony/http-foundation":"2.0.*",
        "twitter/bootstrap":"dev-master",
        "nocarrier/hal":"dev-master",
        "zendframework/zend-log":"2.0.*"
    },
    "require-dev":{
        "printo/printo":"*",
        "facebook/xhprof":"0.9.*",
        "firephp/firephp-core":"dev-master",
        "twig/twig":">=1.0.0"
    },
    "autoload":{
        "psr-0":{
            "BEAR\\Package":"src/",
            "BEAR\\Installer":"src/",
            "Helloworld\\":"apps/",
            "Sandbox\\":"apps/",
            "Skeleton\\":"apps/"
        }
    },
    "minimum-stability":"dev",
    "scripts"   :{
        "post-install-cmd": "BEAR\\Installer\\Composer::postInstall"
    },
    "repositories":[
        {
            "type":"composer",
            "url":"http://packages.zendframework.com/"
        },
        {
            "type":"package",
            "package":{
                "name":"crodas/haanga",
                "version":"1.0.4",
                "dist":{
                    "url":"https://github.com/crodas/Haanga/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"https://github.com/crodas/Haanga.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"facebook/xhprof",
                "version":"0.9.2",
                "dist":{
                    "url":"https://github.com/facebook/xhprof/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"https://github.com/facebook/xhprof.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "version":"master",
                "name":"twitter/bootstrap",
                "source":{
                    "url":"https://github.com/twitter/bootstrap.git",
                    "type":"git",
                    "reference":"master"
                },
                "dist":{
                    "url":"https://github.com/twitter/bootstrap/zipball/master",
                    "type":"zip"
                }
            }
        }
    ]
}
